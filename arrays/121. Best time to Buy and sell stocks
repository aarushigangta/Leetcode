class Solution {
    public int maxProfit(int[] prices) {

        int mp=0;
        int bb=prices[0];

        for(int i=0;i<prices.length;i++){
            if(mp<prices[i]){
                mp=Math.max(mp,prices[i]-bb);
            }
            bb=Math.min(bb,prices[i]);
        }
        
        return mp;
    }
}
OR
class Solution {
    public int maxProfit(int[] prices) {
        int profit=0;
        int buy=prices[0];

        for(int i=0;i<prices.length;i++){
            if(prices[i]<buy){
                buy=prices[i];
            }
            else if(prices[i]-buy>profit){
                profit=prices[i]-buy;
            }
        }
        return profit;
    }
}
