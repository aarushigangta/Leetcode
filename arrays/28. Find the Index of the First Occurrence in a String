class Solution {
    public int strStr(String haystack, String needle) {
       if(needle.length()==0) return 0;
       if(needle.length()> haystack.length()){
        return -1;
       }


       int k= needle.length();
       int n=haystack.length();

       for(int i=0;i<=n-k;i++){
        String window = haystack.substring(i,i+k);
        
        if(window.equals(needle)){
        return i;
        }
       }
       return -1;
    }
}
